{
  "name": "Nimble",
  "version": "2.0.0",
  "description": "A Wolfram|AlphaÂ® menu bar app",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "auto-launch": "^5.0.1",
    "electron-is-dev": "^0.1.2",
    "electron-settings": "^3.0.14",
    "menubar": "^5.2.3",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "bugsnag-js": "^3.2.0",
    "classname": "^0.0.0",
    "color": "^1.0.3",
    "concurrently": "^3.4.0",
    "electron": "^9.4.0",
    "electron-builder": "^17.1.1",
    "font-awesome": "^4.7.0",
    "jacobmarshall-react-inline-svg": "^2.0.1",
    "normalize.css": "^5.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-scripts": "0.9.3"
  },
  "scripts": {
    "clean": "rm -rf ./{bundle,dist}",
    "start": "concurrently \"npm run -s watch\" \"npm run -s electron\"",
    "watch": "NODE_PATH=src/ react-scripts start",
    "build": "NODE_PATH=src/ react-scripts build && mv build bundle",
    "test": "NODE_PATH=src/ react-scripts test --env=jsdom",
    "electron": "NODE_ENV=development electron .",
    "dist": "npm run -s build && build --publish onTagOrDraft"
  },
  "author": {
    "name": "Maybulb",
    "email": "team@maybulb.com",
    "url": "https://maybulb.com"
  },
  "build": {
    "appId": "com.maybulb.nimble",
    "mac": {
      "target": "default",
      "icon": "src/assets/app/nimble.icns"
    },
    "dmg": {
      "title": "Nimble",
      "icon": "src/assets/app/nimble.icns",
      "iconSize": 80,
      "background": "src/assets/app/background.png",
      "contents": [
        {
          "x": 438,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file"
        }
      ]
    }
  },
  "homepage": "./"
}
